BEGIN:VCALENDAR
PRODID:-//SPy, o.z.//calendar generated by pycon.sk//EN
VERSION:2.0
CALSCALE:GREGORIAN
METHOD:PUBLISH
X-WR-CALNAME:PyCon SK 2018
X-WR-TIMEZONE:Europe/Vienna
X-WR-CALDESC:Calendar for PyconSK 2018 event{% for event in events %}
BEGIN:VEVENT
DTSTART:{{ event['dtstart'] }}
DTEND:{{ event['dtend'] }}
DTSTAMP:{{ event['dtstamp'] }}
UID:{{ event['uid'] }}
CREATED:{{ event['created'] }}
DESCRIPTION:{{ event['description'] }}
LAST-MODIFIED:{{ event['last-modified'] }}
LOCATION:{{ event['location'] }}
SEQUENCE:{{ event['sequence'] }}
STATUS:{{ event['status'] }}
SUMMARY:{{ event['summary'] }}
TRANSP:{{ event['transp'] }}
END:VEVENT{% endfor %}
END:VCALENDAR